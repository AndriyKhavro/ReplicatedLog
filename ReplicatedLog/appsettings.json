{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "ReplicationLog": {
    "MaxAppendDelayMs": 0,
    "Secondaries": [ "http://localhost:38081", "http://localhost:38082" ]
  }
}
